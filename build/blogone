<h4>Javascript</h4>
<p>```javascript
function test(){
    console.log("Hello world!");
}</p>
<p>(function(){
    var box = function(){
        return box.fn.init();
    };</p>
<div class="codehilite"><pre><span></span><code><span class="n">box</span><span class="p">.</span><span class="n">prototype</span> <span class="o">=</span> <span class="n">box</span><span class="p">.</span><span class="n">fn</span> <span class="o">=</span> <span class="err">{</span>
    <span class="n">init</span> <span class="p">:</span> <span class="k">function</span><span class="p">()</span><span class="err">{</span>
        <span class="n">console</span><span class="p">.</span><span class="n">log</span><span class="p">(</span><span class="s1">&#39;box.init()&#39;</span><span class="p">);</span>

        <span class="k">return</span> <span class="n">this</span><span class="p">;</span>
    <span class="err">}</span><span class="p">,</span>

    <span class="k">add</span> <span class="p">:</span> <span class="k">function</span><span class="p">(</span><span class="n">str</span><span class="p">)</span><span class="err">{</span>
        <span class="n">alert</span><span class="p">(</span><span class="ss">&quot;add&quot;</span><span class="p">,</span> <span class="n">str</span><span class="p">);</span>

        <span class="k">return</span> <span class="n">this</span><span class="p">;</span>
    <span class="err">}</span><span class="p">,</span>

    <span class="n">remove</span> <span class="p">:</span> <span class="k">function</span><span class="p">(</span><span class="n">str</span><span class="p">)</span><span class="err">{</span>
